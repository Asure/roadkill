#
# USED TO MAKE MASTER EQUATE FILE AP.H
#
EQUS =	DISPEQU.ASM MPROCEQU.ASM APSEQU.ASM SYSEQU.I APEQU.ASM APMEQU.ASM \
	APDIP.ASM APAUDN.ASM 
MEQUS =	DISPEQU.ASM+MPROCEQU.ASM+APSEQU.ASM+SYSEQU.I+APEQU.ASM+APMEQU.ASM+APDIP.ASM+APAUDN.ASM

OBJS =	AP.OBJ DRATT.OBJ DRB.OBJ APC.OBJ APD.OBJ DRRAM.OBJ \
	DR.OBJ DRINP.OBJ DRSINCOS.OBJ DRI.OBJ DRH.OBJ DRT.OBJ DRS.OBJ \
	DRU.OBJ DRARENA1.OBJ DRARENA2.OBJ DRARENA3.OBJ DRARENA4.OBJ \
	DRARENA5.OBJ DRARENA6.OBJ DRARENA7.OBJ DRARENA8.OBJ \
	DRA.OBJ DRAS.OBJ DRC.OBJ \
	DRR.OBJ DRD.OBJ DRSND.OBJ DRM.OBJ DRMIS.OBJ DRACT.OBJ DRBAR.OBJ \
	DRCHRSEL.OBJ DRINITAL.OBJ DREQUIP.OBJ JSTRING.OBJ DRWINSCR.OBJ \
	DRFLUFF.OBJ DRFOUR.OBJ \
	BGNDTBL.OBJ BGNDPAL.OBJ \
	IMGPAL.OBJ IMGTBLM1.OBJ IMGTBLM2.OBJ IMGTBLB1.OBJ IMGTBLF1.OBJ \
	IMGTBLC1.OBJ IMGTBLC2.OBJ

DOBJS =	APADJ.OBJ APCMOS.OBJ APCOIN.OBJ APDTST.OBJ APDUTL.OBJ \
	APMENU.OBJ SYSFONT.OBJ APWORD.OBJ \

SOBJS =	APMISC.OBJ APPALL.OBJ APPROC.OBJ APSND.OBJ APTEXT.OBJ \
	APTV.OBJ APUTIL.OBJ T2PAL.OBJ

.asm.obj:
        @ ECHO *----assembling----$*.ASM(0)
        GSPA $* $*.OBJ $(TVPATH)$*.LST -H -L -v10
	CRNCHLST $(TVPATH)$*.LST

$(TVPATH)AP.OUT: AP.H $(OBJS) $(TVPATH)APSYSTEM.RLO $(TVPATH)APDIAG.RLO
	GLNKL AP

$(TVPATH)APDIAG.RLO: $(DOBJS)
	GLNK APDIAG

$(TVPATH)APSYSTEM.RLO: $(SOBJS)
	GLNK APSYSTEM

AP.H:	$(EQUS)
        @ ECHO *** UPDATING MERGED EQUATE FILE -- AP.H ***
	COPY $(MEQUS) AP.h

$(OBJS):	$*.ASM
$(DOBJS):	$*.ASM
$(SOBJS):	$*.ASM
AP.OBJ		AP.H STRING.H DRPRO.I DRDISP.I
APTV.OBJ:	AP.H
APPROC.OBJ:	AP.H DRDISP.I
APD.OBJ:	AP.H DRFGHOST.I
